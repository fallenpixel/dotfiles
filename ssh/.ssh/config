
##PERSONAL
#Kingslanding
Host kingslanding
    HostName kingslanding
    ForwardX11 yes
    ForwardX11Trusted yes
    ForwardAgent yes
    User katyl
    IdentityFile  ~/.ssh/personal_rsa

#Pyke
Host pyke
    HostName pyke
    ForwardX11 yes
    ForwardX11Trusted yes
    ForwardAgent yes
    User katyl
    IdentityFile  ~/.ssh/personal_rsa

##INFRASTRUCTURE
#westeros
Host westeros
    HostName westeros
    User katyl
    IdentityFile  ~/.ssh/inf_rsa

#essos
Host essos
    HostName essos
    User katyl
    IdentityFile  ~/.ssh/inf_rsa

#Casterlyrock
Host casterlyrock
    HostName casterlyrock
    User katyl
    IdentityFile  ~/.ssh/inf_rsa
    ForwardAgent yes

##DIGITALOCEAN SERVERS
#Droplet
Host webhost
    HostName katyl.info
    User katyl
    ForwardAgent yes
    IdentityFile  ~/.ssh/digitalocean_rsa

#MailHost
Host mailhost
    Hostname mail.katyl.info
    User katyl
    ForwardAgent yes
    ForwardX11 no
    IdentityFile  ~/.ssh/digitalocean_rsa

##GIT SERVICES
#github
Host github
    HostName github.com
    User git
    ForwardX11 no
    IdentityFile  ~/.ssh/git_rsa
    MACs hmac-sha2-512-etm@openssh.com,hmac-sha2-256-etm@openssh.com,hmac-sha2-512
    
#gogs
Host gogs
    User git
    HostName casterlyrock.westeros
    Port 10022
    ForwardX11 no
    IdentityFile  ~/.ssh/git_rsa

#AUR
host aur
    HostName aur.archlinux.org
    User aur
    ForwardX11 no
    IdentityFile ~/.ssh/git_rsa

#Default Options
Host *
    AddressFamily any
    ControlMaster auto
    ControlPath ~/.ssh/socket-%r@%h:%p
    ControlPersist yes
    HashKnownHosts yes
    FallBackToRsh no
    ServerAliveInterval 30
    ServerAliveCountMax 5
    ConnectTimeout 30
    KexAlgorithms curve25519-sha256@libssh.org,ecdh-sha2-nistp521,ecdh-sha2-nistp384,ecdh-sha2-nistp256,diffie-hellman-group-exchange-sha256
    MACs hmac-sha2-512-etm@openssh.com,hmac-sha2-256-etm@openssh.com,umac-128-etm@openssh.com,hmac-sha2-512,hmac-sha2-256,umac-128@openssh.com
    Ciphers chacha20-poly1305@openssh.com,aes256-gcm@openssh.com,aes128-gcm@openssh.com,aes256-ctr,aes192-ctr,aes128-ctr
