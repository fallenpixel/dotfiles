IdentityFile ~/.ssh/git_rsa

#Default Options
ConnectTimeout 10
Host *
HashKnownHosts yes
FallBackToRsh no
KeepAlive yes
AddressFamily any
Compression yes
ServerAliveInterval 10
HashKnownHosts yes
ConnectTimeout 30
ServerAliveInterval 10
ControlMaster auto
ControlPersist yes
        
