# PERSONAL
    ## Personal Devices
        # Pyke 
            Host pyke
            HostName pyke
            User katyl
            # IdentityFile ~/.ssh/id_ed25519
            ForwardAgent yes
            ForwardX11 yes
	    RequestTTY yes

        # Shadow
            Host shadow
            HostName shadow
            User katyl
            # IdentityFile ~/.ssh/id_ed25519
            ForwardAgent yes
            ForwardX11 yes
	    RequestTTY yes
    #
    ## INFRASTRUCTURE
        # westeros
            Host westeros router
            HostName westeros
            User katyl
            # IdentityFile  ~/.ssh/inf_rsa

        # essos
            Host essos wifi
            HostName essos
            User root
            # IdentityFile  ~/.ssh/inf_rsa

        # Casterlyrock
            Host casterlyrock
            HostName casterlyrock
            User katyl
            # IdentityFile  ~/.ssh/inf_rsa
            ForwardAgent yes
            ForwardX11 yes
	    RequestTTY yes

# GIT SERVICES
    ## github
        Host github github.com
        HostName github.com
        User git
        ForwardX11 no
        # IdentityFile  ~/.ssh/git_rsa
        MACs hmac-sha2-512-etm@openssh.com,hmac-sha2-256-etm@openssh.com,hmac-sha2-512
    
    ## AUR
        host aur aur.archlinux.org
        HostName aur.archlinux.org
        User aur
        ForwardX11 no
        # IdentityFile ~/.ssh/git_rsa

# School
    # Thunder- For Shells
        host thunder thunder.cise.ufl.edu
        hostname thunder.cise.ufl.edu
        User elehmann
        # IdentityFile ~/.ssh/id_ed25519.pub
        RequestTTY yes
	ForwardAgent yes
        RemoteCommand '/usr/bin/zsh'

    ## Storm-  for SCP
        host storm storm.cise.ufl.edu
        hostname storm.cise.ufl.edu
        User elehmann
        # IdentityFile ~/.ssh/id_ed25519.pub

# Virtual Machines
    # Kali linux installation
        host kali 
        hostname kali
        user katyl
        # IdentityFile ~/.ssh/inf_rsa
        ProxyJump shadow
	RequestTTY yes

    # Win10 installation
        host win10 
        hostname win10
        user katyl
        IdentitiesOnly = yes
        # IdentityFile ~/.ssh/inf_rsa
        ProxyJump shadow

 
 # Ubuntu 20.04 installation
        host ubuntu 
        hostname ubuntu
        user katyl
        # IdentitiesOnly = yes
        # IdentityFile ~/.ssh/inf_rsa
        ProxyJump shadow 
	RequestTTY yes

# Reptilian forf OS
	host reptilian
	hostname reptilian
	user reptilian
	ProxyJump shadow
	RequestTTY yes
	ForwardAgent yes


#Default Options
Host *
    AddressFamily any
    HashKnownHosts yes
    FallBackToRsh no
    RequestTTY  yes
    ServerAliveInterval 30
    ServerAliveCountMax 5
    ConnectTimeout 30
    KexAlgorithms curve25519-sha256@libssh.org,ecdh-sha2-nistp521,ecdh-sha2-nistp384,ecdh-sha2-nistp256,diffie-hellman-group-exchange-sha256,diffie-hellman-group14-sha256
    MACs hmac-sha2-512-etm@openssh.com,hmac-sha2-256-etm@openssh.com,umac-128-etm@openssh.com,hmac-sha2-512,hmac-sha2-256,umac-128@openssh.com,hmac-sha1
    Ciphers chacha20-poly1305@openssh.com,aes256-gcm@openssh.com,aes128-gcm@openssh.com,aes256-ctr,aes192-ctr,aes128-ctr
